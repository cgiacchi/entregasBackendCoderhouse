<div class="products" id="home">
  <h1>Carrito</h1>
    {{#each products}}
      <div class="product__card">
        <img src="/assets/{{this.product.thumbnail}}" alt="{{this.title}}" />
        <h2>{{this.product.title}}</h2>
        <h3>{{this.product.category}}</h3>
        <p>U$S{{this.product.price}}</p>
        <p>Quantity:{{this.quantity}}</p>
      </div>
    {{/each}}
</div>

